{
    "name": "inkvizitoria/laravel-monoparts",
    "description": "Laravel package for monobank \"Покупка частинами\" integration (order lifecycle, callbacks, signing, logging) by Drozh Denis (DD).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Drozh Denis (DD)",
            "email": "dd@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/log": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.6|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Inkvizitoria\\\\MonoParts\\\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Inkvizitoria\\\\MonoParts\\\\Tests\\\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Inkvizitoria\\\\MonoParts\\\\Providers\\\\MonoPartsServiceProvider"
            ],
            "aliases": {
                "MonoParts": "Inkvizitoria\\\\MonoParts\\\\Facades\\\\MonoParts"
            }
        }
    }
}
